{
  "root": true,
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  },
  "plugins": [
    "babel"
  ],
  "rules": {
    "no-unused-vars": ["error", { "args": "none" }]
  }
}
